{
  "manifest_version": 3,
  "name": "Bconomy Inventory Value Calculator",
  "version": "1.0",
  "description": "Calculates total base and market value of all owned items in Bconomy.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "*://*.bconomy.net/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.bconomy.net/*"],
      "js": ["content.js"]
    }
  ]
}